// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
model User {
  id Int @id @default(autoincrement())
  Name String
  Email String @unique
  Password String
  Score Int
}
model Product{
  id Int  @id @default(autoincrement())
  Name String
  Statut Int @default(1)
  Description String
  SellerId Int @unique
  NbBaskets Int
}
model Basket{
  id Int @id @default(autoincrement())
  ProdutsList Int[]
}
model Command{
  id Int @id
  UserId Int @unique
  ProductsList Int[]
}
model Categorie{
  id Int @id @default(autoincrement())
  Score Int
  ProductsList Int[]
  Stock Int
}